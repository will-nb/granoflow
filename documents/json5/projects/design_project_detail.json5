        position: "进度条下方",
        note: "显示具体的完成情况和百分比",
        // 增强：数字变化动画
        animation: {
          enabled: true,
          type: "countUp",
          duration: 500, // 毫秒
          curve: "Curves.easeOut",
          note: "数字变化时使用计数动画，增强视觉反馈",
        },
      },
      
      // 增强：可选进度环形图（未来扩展）
      progressRing: {
        enabled: false, // 默认不启用，未来可扩展
        component: "CircularProgressIndicator",
        size: 64, // dp
        strokeWidth: 6, // dp
        color: "Theme.of(context).colorScheme.primary",
        backgroundColor: "Theme.of(context).colorScheme.surfaceVariant",
        note: "可选：使用环形图显示进度，提供更直观的视觉体验。当前使用进度条，未来可考虑添加此选项",
        animation: {
          enabled: true,
          duration: 600, // 毫秒
          curve: "Curves.easeOut",
        },
      },
      
      // 增强：进度状态图标
      statusIcon: {
        enabled: true,
        icons: {
          completed: "Icons.check_circle",
          inProgress: "Icons.trending_up",
          notStarted: "Icons.radio_button_unchecked",
        },
        size: 16, // dp
        position: "进度文字右侧",
        note: "根据进度状态显示不同的图标：已完成（100%）、进行中（0-99%）、未开始（0%）",
      },
    },
    
    note: "项目信息头部组件，显示项目标题、基本信息和整体进度",
  },
  
