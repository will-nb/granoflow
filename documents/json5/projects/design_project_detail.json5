    },
    
    // 项目进度可视化
    progressVisualization: {
      enabled: true,
      
      // 进度条（主要显示方式）
      progressBar: {
        component: "LinearProgressIndicator",
        height: 6, // dp
        borderRadius: 3, // dp
        color: "Theme.of(context).colorScheme.primary",
        backgroundColor: "Theme.of(context).colorScheme.surfaceVariant",
        note: "显示项目整体进度（已完成任务数 / 总任务数）",
        // 增强：进度条动画
        animation: {
          enabled: true,
          duration: 300, // 毫秒
          curve: "Curves.easeOut",
          note: "进度变化时使用平滑过渡动画，提升视觉体验",
        },
      },
      
      // 进度文字
      progressText: {
        enabled: true,
        format: "进度：已完成 X / 总计 Y 个任务（Z%）",
